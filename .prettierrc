{
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "printWidth": 80,
  "plugins": ["prettier-plugin-tailwindcss"],
  "quoteProps": "as-needed",
  "trailingComma": "all",
  "bracketSpacing":  true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "strict",
  "endOfLine": "lf",
  "singleAttributePerLine": false
}
